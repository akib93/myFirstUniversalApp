<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:IDB_BISEW">

    <ImageBrush x:Key="HospitalBac" ImageSource="Image/untitled.png"/>
    <ImageBrush x:Key="Idb" ImageSource="Image/thWNXR2VCZ.jpg"/>
    

    <Style x:Key="GridStyle" TargetType="Grid">
        <Setter Property="Background" Value="{StaticResource HospitalBac}"/>
    </Style>


    <Style x:Key="GridStyle2" TargetType="Grid">
        <Setter Property="Background" Value="{StaticResource Idb}"/>
    </Style>
</ResourceDictionary>
